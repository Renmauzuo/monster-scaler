	"version": "0.1.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "development",
			"program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
			"env": {
				"NODE_ENV": "development"
			},
			"args": [
				"default"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "production",
			"program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
			"env": {
				"NODE_ENV": "production"
			},
			"args": [
				"rebuild"
			]
		}
	]
}
